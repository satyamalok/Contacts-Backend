{
  "name": "contacts-sync-backend",
  "version": "1.0.0",
  "description": "Real-time contacts sync backend for Android sales team",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "migration:create": "node -r tsx/cjs database/migrations/create.js",
    "migration:run": "node -r tsx/cjs database/migrations/run.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "contacts",
    "sync",
    "websocket",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.2",
    "fastify-socket.io": "^5.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "papaparse": "^5.4.1",
    "nanoid": "^3.3.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/papaparse": "^5.3.14",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
